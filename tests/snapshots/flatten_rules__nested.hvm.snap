(Rule (CtrA a x$0)) = (Rule$F0 a x$0)
(Rule (CtrB b)) = b
(Rule x) = x

(CtrB1) = λb λ#Bar CtrB1 λ#Bar CtrB2 (#Bar.CtrB1.b CtrB1 b)

(CtrB2) = λa λb λ#Bar CtrB1 λ#Bar CtrB2 (#Bar.CtrB2.b (#Bar.CtrB2.a CtrB2 a) b)

(CtrC) = λ#Baz CtrC CtrC

(CtrA) = λa λb λ#Foo CtrA λ#Foo CtrB (#Foo.CtrA.b (#Foo.CtrA.a CtrA a) b)

(CtrB) = λa λ#Foo CtrA λ#Foo CtrB (#Foo.CtrB.a CtrB a)

(Rule$F0 a (CtrB1 b)) = (a b)
(Rule$F0 a (CtrB2 x$0 b)) = (Rule$F0$F0 a x$0 b)
(Rule$F0 a b) = (a b)
(Rule$F0 x$0 x$1) = (CtrA x$0 x$1)

(Rule$F0$F0 a (CtrC) b) = (a b)
(Rule$F0$F0 a x$0 x$1) = (a (CtrB2 x$0 x$1))
(Rule$F0$F0 x$0 x$0 x$1) = (CtrA x$0 (CtrB2 x$0 x$1))
